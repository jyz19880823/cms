*{margin:0;padding:0;}
body{font: 12px Verdana, Arial, Helvetica, sans-serif;}
a{text-decoration:none;color:#333;}
a:hover{background: none;}
a img{border:none;}
.clear{clear:both;}
.wrapper{width:1000px;border:1px #aaa solid;margin:0 auto;
box-shadow: 0 0 8px rgba(10, 10, 10, 10);
-webkit-box-shadow: 0 0 8px rgba(10, 10, 10, 10);
-moz-box-shadow: 0 0 8px rgba(10, 10, 10, 10);}
.float-right{float:right;}
.float-left{float:left;}
.head{}
.head-top{background:#eeeeee;padding:10px 50px;border-bottom:1px #aaa solid;color:#888;font-size:11px;}
.head-menu{border-bottom:1px #aaa solid;padding:10px 0;}
.head-menu ul{list-style:square;padding-left:50px;padding-right:50px;color:#aaa;margin:0}
.head-menu ul li{float:left;margin:0 25px 0 15px;_margin:0 45px 0 0 ;}
.head-menu a{text-decoration:none;color:#555;}
.head-menu li:hover a{text-decoration:none;color:#000;}
.head-menu .active a{text-decoration:none;color:#000;}
.head-bottom{padding-top:30px;}
.logo-wrapper{width:400px;float:left;color:#999;margin-left:50px;_margin-left:25px;}
span.logo{font-size:23px;}
.logo a{color:#0067ca;}
.search-form{float:left;margin-left:200px;}
.search-form span{color:#999;font-size:11px;}

/*********zhong*********/
.body{padding:0 50px;margin-top:20px;}
.banner{margin-bottom:20px}
.banner-text{width:200px;position:absolute;margin-top:25px;margin-left:700px;z-index:2000;font-size:16px;}
.banner-text a{color:#fff;}
.banner img{z-index:-2000}
.index h1{font-size:20px;display:inline;}
.content{color:#444;line-height:16px;font-size:11px;}
.index-more{display:inline;margin-left:150px;}
.index-more img{vertical-align:middle;}
.index blockquote{border-bottom:1px #aaa solid;padding-bottom:19px;margin-top:15px;}
.case-head img{margin-top:20px;}
.index-case h4{margin-top:20px;margin-bottom:15px;width:730px;float:left;}
.index-gallery img{margin-right:30px;border:1px #aaa solid;padding:3px;}
.index-gallery-in{margin-right:20px;display:block;width:200px;float:left;}
.index-gallery-in img{margin-bottom:15px;padding:2px;border:1px #ccc solid;}
.index-gallery-in h3{margin-bottom:5px;}
.index-gallery {border-bottom:1px #aaa solid;padding-bottom:20px;}

/*************footer********************/
.footer{padding-left:50px;background:#dedede;margin-top:40px;padding-top:20px;padding-bottom:10px;_margin-top:20px;_padding-top:10px;}
.footer-in{float:left;margin-right:10px;width:200px;margin-top:-25px;_margin-top:0;}
.footer-in h5{line-height:20px;padding-bottom:5px;color:#444;margin:-10px 0 10px 0;}

.footer-in ul{list-style:square;color:#aaa;padding-left:15px;}
.footer-in ul li{line-height:20px;}
.footer-in ul li a{color:#333;}
.footer-message{padding:10px 0;background:#333;padding-left:50px;}
p.footer-hb{border-top:1px #aaa solid;border-bottom:1px #fff solid;margin-top:20px;height:0;_border-top:none;_margin-bottom:-40px;width:800px;float:left;}

/**********************list*************************/
.navigation{border-top:1px #aaa solid;border-bottom:1px #aaa solid;margin:10px 0 0 0;padding:7px 50px;_padding:3px 0 6px 50px;background:#f6f6f6}
a.gray{color:#aaa;}
.navigation a.black{color:#000;}
.navigation a{color:#555;}
.main{padding-left:50px;}
.main-left{width:630px;float:left;}
.product{margin-top:20px;}
.product h2{border-bottom:1px #ccc dotted;padding-bottom:5px;margin-bottom:5px;font-weight:normal;}
.product h2 a{color:#444}
.product p a {color:#f00}
.product p span{margin-right:30px;}
.product img{float:left;margin-right:10px;margin-bottom:10px;}
.product blockquote{border:none;font-size:11px;color:#777;line-height:17px;}
.read-more img{margin:0;margin-top:-6px}
.read-more {background:url(image/list-bg.jpg) repeat-x left top ;padding:8px 8px 4px 8px;border:1px #aaa solid;}
.quotation-list img{margin-right:5px;}

.main-right{border-left:1px #aaa solid;float:right;width:300px;background:#fcfcfc;}
.main-right h3{border-bottom:1px #aaa dashed;margin-left:10px;padding-bottom:5px;margin-top:20px;margin-right:20px;color:#555;}
.main-right ul {list-style:none;margin-left:10px;margin-right:20px;}
.main-right ul li{border-bottom:1px #aaa dashed;padding:5px;padding-bottom:2px;}
.main-right ul li a{color:#666;font-size:11px;}
ul.hover-li li:hover a{color:#0067ca;}
ul.hover-a li a:hover{color:#0067ca;}


.main-right p{padding:5px;margin-left:10px;color:#666;font-size:11px;margin-right:25px;}
.pro-img{margin-top:15px;margin-bottom:15px;}

a.join{font-size:11px;color:#0067ca;}
/**************************************************************/
.inside h1{float:left;width:600px;margin-top:20px;}
.inside-content-left{width:320px;border:1px #ddd dashed;float:left;margin-top:20px;padding:10px;margin-bottom:10px;}
.content p{margin-bottom:10px;border-bottom:1px #aaa dashed;padding-bottom:10px;}
p.content-last{border-bottom:none;}

.img_buy{float:right;}
.buy_now{margin-top:5px;}
.crusher-h2{font-size:11px;color:#275392;margin-bottom:5px;}
.blue{color:#0067ca;}
.inside-content-right{width:230px;font-size:11px;margin-top:20px;margin-left:40px;}
.rate{margin-top:10px;}
.rate img{vertical-align:middle;}
.comment{margin-top:15px;}
.comment-content{padding-top:10px;}
.related{margin-top:15px;}
ul.related-content , ul.keyword-content{list-style:none;padding-top:5px;padding-bottom:15px;color:#777}
ul.related-content a{color:#0067ca;}
.add{color:#444;margin-top:10px;margin-bottom:10px;}



.inside-content-bottom{clear:both;font-size:11px;}
#tb{color:#444;font-size:11px;border-collapse:collapse;border-color:#aaa;}
#tb tr td{border-color:#aaa;padding:5px}
.odd{background:#ccc;}
.even{background:#eee;}
.tb_th{}

.inside-content-bottom img{margin-top:25px;}
#Form1{margin-top:20px;width:400px;float:left;}
#Form1 div{margin:5px 0;}
#Form1 input, #Form1 textarea{width:180px;border:1px #ccc solid;border-collapse:collapse;padding-right:20px;}
.form-left{float:left;width:150px;padding:0;}
#Form1 input.menu2{width:50px;margin-left:150px;}
hr{border:none;border-bottom:#aaa solid 1px;margin-top:10px;margin-bottom:10px;}
.backtop{vertical-align:middle;}
input.yes, textarea.yes{background:url(image/yes.gif) right top no-repeat;}
input.no, textarea.no{background:url(image/no.gif) right top no-repeat;}


/******************contact**********************/
.contact {font-size:11px;color:#333;}
.contact h1{margin-bottom:5px;}
.contactin h2{background:url(image/cont.gif) no-repeat left top;padding-left:25px;margin-top:20px;color:#333;margin-bottom:5px;}
.contactin p{padding-left:25px;border-bottom:1px #aaa dotted;color:#333;padding-bottom:10px;}
a.depart {line-height:18px;color:#0067ca}
.depart img{vertical-align:middle;margin:0 5px;}
.contact #Form1 p{margin:0;padding:0;border:none;}
/**************customize***/
.customize{color:#444;}
.customize h1{margin-bottom:10px;}
.customizetin{margin-top:20px;}
.customize h3{margin-top:10px}
.customize p{margin-top:5px;}
.customize table{border:1px #aaa solid;width:100%;margin-top:5px;}
.customizeguidec{background:url(image/customizeguidec.jpg) no-repeat right top;height:88px;border:1px #aaa solid;padding:6px 10px;}
.hid{display:none;}
.customizetin #Form1{margin:0;padding:0;}
.form1left{width:200px;text-align:right;}
.form1right{width:270px;text-align:left;padding:5px;}
.form1right input {width:250px;height:20px;border-collapse:collapse;border:1px #ddd solid;background:url(image/inputbg.gif) repeat-x top left;}
.form1right textarea{width:250px;height:100px;border-collapse:collapse;border:1px #ddd solid;background:#efefef;}

/**********beneficiaiton*******************/

.bene-img img{width:200px;height:auto;margin:5px;}
.bene-img{float:left;border:1px #aaa solid;margin-right:5px;margin-bottom:5px;}

/*****************processs***********/
.div{padding-top:40px;color:#666;font-size:11px;line-height:15px;}
.div .process-img{margin:20px;margin-top:0;}
.div .project-img{margin:10px;margin-top:0;}

/*************************project******/
.project_table{border-collapse:collapse;margin-top:20px;}
.project_table tr:hover{background:#aaa;}
.project_td_left{width:200px;border:1px dotted #aaa;height:25px;padding-left:20px;}
.project_td_right{width:300px;border:1px dotted #aaa;height:25px;padding-left:20px;}